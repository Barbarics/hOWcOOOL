	Speed = .9 -- 1 is pretty much good but you do you :D I have high ping so you mighy need lower
	_G.Enable = true



	UIS = game:GetService("UserInputService")
	local UserInputService = game:GetService("UserInputService")

	while _G.Enable do
		if UIS:IsKeyDown(Enum.KeyCode.W) then
			game.Players.LocalPlayer.Character.HumanoidRootPart.Anchored = true
			game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game:GetService("Workspace").MapFolder.Players[YourUsername].HumanoidRootPart.CFrame * CFrame.new(0,0,-Speed)
			game.Players.LocalPlayer.Character.HumanoidRootPart.Anchored = false
			keypress(0x11)
			wait(0.01)
			keyrelease(0x11)
		end
		if UIS:IsKeyDown(Enum.KeyCode.A) then
			game.Players.LocalPlayer.Character.HumanoidRootPart.Anchored = true
			game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game:GetService("Workspace").MapFolder.Players[YourUsername].HumanoidRootPart.CFrame * CFrame.new(-Speed,0,0)
			game.Players.LocalPlayer.Character.HumanoidRootPart.Anchored = false
			keypress(0x11)
			wait(0.01)
			keyrelease(0x11)
		end
		if UIS:IsKeyDown(Enum.KeyCode.S) then
			game.Players.LocalPlayer.Character.HumanoidRootPart.Anchored = true
			game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game:GetService("Workspace").MapFolder.Players[YourUsername].HumanoidRootPart.CFrame * CFrame.new(0,0,Speed)
			game.Players.LocalPlayer.Character.HumanoidRootPart.Anchored = false
			keypress(0x11)
			wait(0.01)
			keyrelease(0x11)
		end
		if UIS:IsKeyDown(Enum.KeyCode.D) then
			game.Players.LocalPlayer.Character.HumanoidRootPart.Anchored = true
			game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game:GetService("Workspace").MapFolder.Players[YourUsername].HumanoidRootPart.CFrame * CFrame.new(Speed,0,0)
			game.Players.LocalPlayer.Character.HumanoidRootPart.Anchored = false
			keypress(0x11)
			wait(0.01)
			keyrelease(0x11)
		end
		wait()
	end
end)

