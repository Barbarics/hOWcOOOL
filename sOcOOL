Tab.Position = UDim2.new(0.364294142, 0, 0.0806962028, 0)
Tab.Size = UDim2.new(0, 436, 0, 529)
Tab.Active = true
Tab.Draggable = true

Frame.Parent = Tab
Frame.Active = true
Frame.BackgroundColor3 = Color3.fromRGB(17, 17, 17)
Frame.BorderColor3 = Color3.fromRGB(61, 61, 61)
Frame.Position = UDim2.new(0.0206422023, 0, 0.0545445159, 0)
