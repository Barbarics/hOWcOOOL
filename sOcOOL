Emotes.MouseButton1Click:Connect(function()
	local animations = {
		["Dophin Dance"] = 5918726674,
		["Applaud"] = 5915693819,
		["Country Line  Dance"] = 5915712534,
		["Floss  Dance"] = 5917459365,
		["Panini Dance"] = 5915713518,
		["Rock On"] = 5915714366,
